{
  "name": "davidverholen/blog",
  "description": "Just another Blog Module for Magento2",
  "require": {
    "php": "^5.5.0|^7.0.0",
    "magento/framework": "^100.0.0",
    "magento/module-backend": "^100.0.0",
    "magento/module-store": "^100.0.0",
    "magento/module-widget": "^100.0.0"
  },
  "require-dev": {
    "n98/magerun2": "^1.1.4",
    "phpunit/phpunit": "~4.1.0",
    "phpspec/phpspec": "^2.4.1",
    "phing/phing": "^2.9.1",
    "composer/composer": "^1.0.0@beta"
  },
  "repositories": {
    "mage.rocks": {
      "type": "composer",
      "url": "https://mage.rocks"
    },
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com"
    }
  },
  "config": {
    "bin-dir": "bin",
    "optimize-autoloader": true
  },
  "type": "magento2-module",
  "license": "OSL-3.0",
  "autoload": {
    "files": [
      "src/registration.php"
    ],
    "psr-4": {
      "DavidVerholen\\Blog\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DavidVerholen\\Blog\\Test\\Integration\\": "test/integration"
    }
  }
}
